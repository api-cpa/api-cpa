<% unless current_service.nil? %>
  <% unless current_proxy.nil? %>
    <% if current_route.nil? %>
    <% else %>
      <div class="b-io-menu--secondary">
        <ul class="list-group">
          <a class="list-group-item" href="<%= service_proxy_route_path(current_service, current_proxy, current_route) %>"><%= t('misc.dashboard') %></a>
          <a class="list-group-item" href="<%= edit_service_proxy_route_path(current_service, current_proxy, current_route) %>"><%= t('actions.edit') %></a>
          <a class="list-group-item" href="<%= service_proxy_route_query_parameters_path(current_service, current_proxy, current_route) %>"><%= t('activerecord.models.query_parameter').pluralize %></a>
        </ul>
      </div>
    <% end %>
  <% end %>
<% end %>
