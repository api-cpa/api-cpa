<% if (data.field) { %>
<h4 class="ui header">
  <i class="<%= data.field.completeType.icon %> icon"></i>
  <div class="content">
    <%= data.field.name %>
    <div class="sub header">Champ</div>
  </div>
</h4>
<% } else { %>
<h4 class="ui header">
  <i class="add icon"></i>
  <div class="content">
    Ajouter un champ
  </div>
</h4>
<% } %>